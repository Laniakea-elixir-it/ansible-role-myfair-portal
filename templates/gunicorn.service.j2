[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory={{ myFAIR_install_path }}
ExecStart={{ myFAIR_install_path }}/.venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:{{ myFAIR_install_path }}/myFAIR.sock myFAIR.wsgi:application

[Install]
WantedBy=multi-user.target
